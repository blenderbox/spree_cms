<%= cms_wrapper(region) do |obj|
  r = ""
  obj.blocks_regions.each do |block_region|
    if block_region.template.present?
      # render the custom thing
      r << render("spree/cms/blocks/#{block_region.template}", block: block_region.block)
    else
      r << render("spree/cms/#{block.class.name.demodulize.tableize}/#{block.class.name.demodulize.underscore}", block: block_region.block)
    end
  end
  r.html_safe
end %>
